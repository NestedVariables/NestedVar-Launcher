{
    "manifest_version": 2,
    "name": "NestedVariables Launcher",
    "version": "1.0.0",
    
    "description": "Launcher for quickly getting to all things NestedVariables",
    "icons": {"128": "icon_128.png"},
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    
    "permissions": ["activeTab"]
}